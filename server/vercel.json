 {
    "version" :2,
    "builds" :[
        {
            "server" : "server.js",
            "use" : "@vercel/node",
            "config" : {
                "includeFiles":[
                    "dist/**"
                ]
            }
        }
    ],
    "routes":[
        {
            "src" : "/(.*)",
            "dest" : "server.js"
        }
    ]
 }